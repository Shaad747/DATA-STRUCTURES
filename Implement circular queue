#include <stdio.h>
#include<stdlib.h>
#define SIZE 5
struct queue
{
    int r,f;
    int data[SIZE];
};
typedef struct queue QUEUE;
void enqueue(QUEUE *q,int item){
    if(q->f==(q->r+1)%SIZE)
        printf("\nQueue is full");
    else{
        q->r=(q->r+1)%SIZE;
        q->data[q->r]=item;
        if(q->f==-1)
            q->f=q->f+1;
    }
}
void dequeue(QUEUE *q){
    int del;
    if(q->f==-1)
        printf("\nQueue is empty");
    else{
        del=q->data[q->f];
        printf("\nThe deleted item is:%d",del);
        if(q->f==q->r){
            q->f=-1;
            q->r=-1;}
        else
            q->f=(q->f+1)%SIZE;
    }
}
void display(QUEUE q){
    int i;
    if(q.f==-1)
        printf("\nQueue is empty");
    else{
        printf("\nQueue content:");
        for(i=q.f;i!=q.r;i=(i+1)%SIZE)
            printf("%d\t",q.data[i]);
    }   printf("%d\t",q.data[i]);
}
int main() {
    int ch,item;
    QUEUE q;
    q.f=-1;
    q.r=-1;
    for(;;){
        printf("\n1.Insert");
        printf("\n2.Delete");
        printf("\n3.Display");
        printf("\nEnter your choice:");
        scanf("%d",&ch);
        switch(ch){
            case 1:printf("\nEnter element to be inserted");
            scanf("%d",&item);
            enqueue(&q,item);
            break;
            case 2:dequeue(&q);
            break;
            case 3:display(q);
            break;
            default:exit(0);
        }
    }
    

    return 0;
}
